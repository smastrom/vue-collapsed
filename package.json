{
	"name": "vue-collapsed",
	"version": "1.1.0",
	"description": "Dynamic CSS height transition from any to auto and vice versa for Vue 3. Accordion ready.",
	"keywords": [
		"vue collapse",
		"vue-collapse",
		"vue accordion",
		"vue-accordion",
		"collapse",
		"accordion",
		"vue component",
		"vue-component"
	],
	"homepage": "https://vue-collapsed.netlify.app/",
	"bugs": {
		"url": "https://github.com/smastrom/vue-collapsed/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/smastrom/vue-collapsed.git"
	},
	"license": "MIT",
	"author": {
		"name": "Simone Mastromattei",
		"email": "smastrom@proton.me"
	},
	"main": "dist/index.umd.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist/*"
	],
	"scripts": {
		"dev": "vite",
		"build": "rimraf dist && run-p type-check build-only",
		"build:app": "vite build --mode app",
		"preview": "vite preview",
		"test": "cypress run --component",
		"test:dev": "cypress open --component",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.cypress-ct.json --composite false",
		"lint": "eslint . --ext .vue,.js,.ts, --fix --ignore-path .gitignore",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{vue,ts,js,cjs,json}": "prettier --write"
	},
	"devDependencies": {
		"@babel/types": "^7.20.7",
		"@rollup/plugin-terser": "^0.2.1",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/tsconfig": "^0.1.3",
		"csstype": "^3.1.1",
		"cypress": "^12.2.0",
		"eslint": "^8.31.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-vue": "^9.8.0",
		"husky": "^8.0.2",
		"lint-staged": "^13.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.9.4",
		"vite": "^4.0.3",
		"vite-plugin-dts": "^1.7.1",
		"vue": "^3.2.45",
		"vue-tsc": "^1.0.19"
	},
	"peerDependencies": {
		"vue": ">=3.0.0"
	}
}
